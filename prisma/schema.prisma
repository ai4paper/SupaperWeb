// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Account {
  id                 String @id @db.VarChar(191)
  userId             String @db.VarChar(191)
  provider           String @db.VarChar(191)
  providerAccountId  String @db.VarChar(191)
  refreshToken       String? @db.VarChar(191)
  accessToken        String? @db.Text
  expiresAt          Int?
  tokenType          String? @db.VarChar(191)
  scope              String? @db.VarChar(191)
  type               String? @db.VarChar(191)
  idToken            String? @db.Text
  sessionState       String? @db.VarChar(191)

  @@index([provider, providerAccountId], name: "accounts_provider_provider_account_id_key")
  @@index([userId], name: "accounts_user_id_idx")
}

model ApiKey {
  id       BigInt   @id @default(autoincrement()) @map("id")
  key      String   @map("key")
  alive    Boolean  @map("alive") @default(true)
  amount   Decimal  @map("amount") @db.Decimal(65, 30)
  used     Decimal  @map("used") @default("0.000000000000000000000000000000") @db.Decimal(65, 30)
  endpoint String   @map("endpoint") @default("https://api.openai.com")
  
  @@map("api_keys")
}
